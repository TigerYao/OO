        return lat;
    }

    public void setLat(String lat) {
        this.lat = lat;
    }

    public String getLng() {
        return lng;
    }

    public void setLng(String lng) {
        this.lng = lng;
    }

    public String getMeetPoint() {
        return meetPoint;
    }

    public void setMeetPoint(String meetPoint) {
        this.meetPoint = meetPoint;
    }


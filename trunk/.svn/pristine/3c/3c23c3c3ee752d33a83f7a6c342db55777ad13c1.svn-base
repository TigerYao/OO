    //任务来源类型:1.甲方直接发布至任务墙;2.查看乙方行程
    private String taskName;
    //任务名称
    private String taskBrief;
    //任务简述
    private String startTime;
    //任务开始时间
    private String endTime;
    //任务结束时间
    private String wallShowTime;
    //任务墙显示时间
    private int wallShowDay;
    //任务墙显示天数
    private int  wallShowHour;
    //任务墙显示小时数
    private int taskAmount;
    //任务金额
    private String countryCode;
    //约会地点国家编码
    private String cityCode;
    //约会地点:即城市编码
    private String lat;

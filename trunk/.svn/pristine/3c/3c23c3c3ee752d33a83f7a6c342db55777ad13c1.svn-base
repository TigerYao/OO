        return cityCode;
    }

    public void setCityCode(String cityCode) {
        this.cityCode = cityCode;
    }

    public String getCompanionProvideDesc() {
        return companionProvideDesc;
    }

    public void setCompanionProvideDesc(String companionProvideDesc) {
        this.companionProvideDesc = companionProvideDesc;
    }

    public int getCompanionQty() {
        return companionQty;
    }

    public void setCompanionQty(int companionQty) {
        this.companionQty = companionQty;

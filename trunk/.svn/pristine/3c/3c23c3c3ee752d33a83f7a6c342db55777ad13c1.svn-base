    private String lng;
    //约会地点:lng
    private String meetPoint;
    //约会地点
    private int  provideStayPlaceFlag;
    //任务提供场所标识:0.否;1.是
    private int payTravelFeeFlag;
    //任务提供旅行费用标识:0.否;1.是
    private int  needDepositeFlag;
    //任务是否需要押金:0.否;1.是
    private String weTodoDesc;
    //我们一起做什么
    private String companionProvideDesc;
    //伴侣需要提供什么
    private String notes;
    private int companionQty;
    //简介
    private CompanionRequirmentRequest companionRequirment;


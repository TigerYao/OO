<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
                android:background="@color/bg_main"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <com.taiwan.oomatcher.widget.SelectableRoundedImageView
        xmlns:app="http://schemas.android.com/apk/res/com.taiwan.oomatcher"
        android:id="@+id/head_image"
        android:layout_width="@dimen/dp_70"
        android:layout_height="@dimen/dp_70"
        android:layout_marginLeft="@dimen/dp_13"
        android:layout_marginTop="@dimen/dp_13"
        android:src="@drawable/default_head"
        android:scaleType="fitXY"
        app:sriv_oval="true"/>
    <TextView
        android:id="@+id/pay_status_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_toRightOf="@id/head_image"
        android:maxLines="1"
        android:textColor="@color/text_fense_normal"
        android:textSize="@dimen/sp_20"
        android:text="@string/companion_item_pay" />
    <TextView
        android:id="@+id/nickname_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_below="@id/pay_status_txt"
        android:layout_toRightOf="@id/head_image"
        android:maxLines="1"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_20" />
    <TextView
        android:id="@+id/remove_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_5"
        android:maxLines="1"
        android:paddingTop="@dimen/dp_5"
        android:paddingBottom="@dimen/dp_5"
        android:textColor="@color/color_5C5D5D"
        android:textSize="@dimen/sp_14"
        android:text="@string/remove" />

    <ImageView
        android:id="@+id/icon_follower"
        android:layout_width="@dimen/dp_11"
        android:layout_height="@dimen/dp_10"
        android:layout_below="@id/head_image"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_22"
        android:scaleType="fitXY"
        android:src="@drawable/image_collection_userinfo" />

    <TextView
        android:id="@+id/tv_followers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/head_image"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_toRightOf="@id/icon_follower"
        android:gravity="center"
        android:textColor="@color/color_A09FB5"
        android:textSize="@dimen/sp_11"
        android:text="1500 Followers" />


    <ImageView
        android:id="@+id/icon_thumbs"
        android:layout_width="@dimen/dp_11"
        android:layout_height="@dimen/dp_10"
        android:layout_below="@id/head_image"
        android:layout_marginLeft="@dimen/dp_91"
        android:layout_marginTop="@dimen/dp_22"
        android:layout_toRightOf="@id/tv_followers"
        android:scaleType="fitXY"
        android:src="@drawable/image_userinfo_thumbs" />

    <TextView
        android:id="@+id/tv_thumbs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/head_image"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_22"
        android:layout_toRightOf="@id/icon_thumbs"
        android:gravity="center"
        android:textColor="@color/color_A09FB5"
        android:textSize="@dimen/sp_11"
        android:text="270 Thumbs" />
    <LinearLayout
        android:id="@+id/infor_ll01"
        android:layout_width="match_parent"
        android:layout_height="@dimen/login_item_height"
        android:orientation="horizontal"
        android:layout_below="@id/icon_follower">
        <TextView
            android:id="@+id/age_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
        <TextView
            android:id="@+id/height_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
        <TextView
            android:id="@+id/weight_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/infor_ll02"
        android:layout_width="match_parent"
        android:layout_height="@dimen/login_item_height"
        android:orientation="horizontal"
        android:layout_below="@id/infor_ll01">
        <TextView
            android:id="@+id/occupation_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
        <TextView
            android:id="@+id/nationality_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
        <TextView
            android:id="@+id/hair_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/infor_ll03"
        android:layout_width="match_parent"
        android:layout_height="@dimen/login_item_height"
        android:orientation="horizontal"
        android:layout_below="@id/infor_ll02">
        <TextView
            android:id="@+id/eye_txt"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_16" />
        <View
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
    </LinearLayout>
    <TextView
        android:id="@+id/language_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/infor_ll03"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_12"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16"
        android:text="1500 Followers" />
    <TextView
        android:id="@+id/accept_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/language_txt"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_12"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16"
        android:text="1500 Followers" />
    <ImageView
        android:id="@+id/accept_image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/language_txt"
        android:layout_toRightOf="@id/accept_txt"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_5"
        android:src="@drawable/duigou"/>
    <TextView
        android:id="@+id/rent_now_txt"
        android:layout_width="match_parent"
        android:layout_height="@dimen/normal_height"
        android:layout_below="@id/accept_txt"
        android:layout_marginTop="@dimen/dp_12"
        android:background="@drawable/shape_task_wall_tab_bg"
        android:gravity="center"
        android:text="@string/rent_now"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16"
        android:orientation="vertical"
        android:paddingBottom="@dimen/dp_5"
        android:paddingTop="@dimen/dp_5"></TextView>
</RelativeLayout>